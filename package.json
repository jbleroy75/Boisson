    "@sentry/nextjs": "^10.38.0",
    "@supabase/supabase-js": "^2.93.3",
    "@vercel/analytics": "^1.6.1",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "react-hot-toast": "^2.6.0",
